{
  "name": "vite-ai-chat-sdk",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "bun run dev:server & bun run dev:client",
    "dev:client": "vite",
    "dev:server": "bun run --watch server/index.ts",
    "build": "bun run build:client && bun run build:server",
    "build:client": "vite build",
    "build:server": "bun build server/index.ts --outdir dist/server --target bun",
    "start": "NODE_ENV=production bun run dist/server/index.js",
    "preview": "vite preview",
    "lint": "bun tsc --noEmit && bunx @biomejs/biome check .",
    "format": "bunx @biomejs/biome check --write .",
    "test": "bun test",
    "test:unit": "bun test lib/",
    "test:e2e": "bunx playwright test",
    "test:coverage": "bun test --coverage"
  },
